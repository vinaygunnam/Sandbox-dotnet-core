@model UiValidation.Models.CheckboxList<string, string>

@for (int i=0; i < Model.ItemsToRender.Count(); i++)
{
    <div class="form-check">
        <input type="checkbox" asp-for="ItemsToRender[i].Selected" />
        <input type="hidden" asp-for="ItemsToRender[i].Value" />
        <label class="form-check-label" for="flexCheckChecked@@i">
            @Model.ItemsToRender[i].Text
        </label>
    </div>
}